<!--/#slider-->
<div class="largebox_content_containerbg">
    <div class="container">

        <div class="container_inner middile_con_inner_bg clearfix">

            <div class="col-md-12 catagory_con_top_bg clearfix">
                <div class="row">
                    <div class="col-md-8">
                        <div class="pull-left">
                            &nbsp;
                        </div>
                        <div class="pull-right category_sort_by_bg">
                        </div>
                    </div>
                    <div class="col-md-4">
                        &nbsp;
                    </div>
                </div><!--row-->
            </div>
            <!-- // catagory_con_top_bg-->
            @if (Model.WithID == "Y")
            {

                <div class="col-md-12 con_8_4_outer_bg clearfix">
                    <div class="row con_row">
                        <div class="col-md-8 home_left_8">
                            <div class="pro_pub_cont_tab_bg mob_padd_bg_25 clearfix">
                                <!--tab div section-->

                                @if (Model.Profile != null)
                                {
                                    if (Model.Profile.UserType == 1)
                                    {
                                        <ul class="nav nav-tabs">
                                            <li class="active" id="licompleted">
                                                <a data-toggle="tab" href="#completed_jobs" aria-expanded="true" id="completedjobcount">
                                                    Total completed jobs
                                                    @if (Model.CompletedJobCount != null)
                                                    {<span id="spcc">(@Model.CompletedJobCount)</span>}
                                                    else
                                                    {<span id="spcc">(0)</span>}
                                                </a>
                                            </li>
                                            <li class="" id="ligigs">
                                                <a data-toggle="tab" href="#Listed_gigs" aria-expanded="false">
                                                    Listed GIGs
                                                    @if (Model.ListedGigCount != null)
                                                    {<span id="splg">(@Model.ListedGigCount)</span>}
                                                    else
                                                    {<span id="splg">(0)</span>}
                                                </a>
                                            </li>
                                        </ul>
                                    }
                                    if (Model.Profile.UserType == 2)
                                    {
                                        <ul class="nav nav-tabs">
                                            <li class="active" id="lilisted">
                                                <a data-toggle="tab" href="#Listed_jobs" aria-expanded="false">
                                                    Listed jobs
                                                    @if (Model.ListedJobCount != null)
                                                    {<span id="splc">(@Model.ListedJobCount)</span>}
                                                    else
                                                    {<span id="splc">(0)</span>}
                                                </a>
                                            </li>
                                        </ul>
                                    }
                                    if (Model.Profile.UserType == 3)
                                    {
                                        <ul class="nav nav-tabs">
                                            <li class="active" id="licompleted">
                                                <a data-toggle="tab" href="#completed_jobs" aria-expanded="true" id="completedjobcount">
                                                    Total completed jobs
                                                    @if (Model.CompletedJobCount != null)
                                                    {<span id="spcc">(@Model.CompletedJobCount)</span>}
                                                    else
                                                    {<span id="spcc">(0)</span>}
                                                </a>
                                            </li>
                                            <li class="" id="lilisted">
                                                <a data-toggle="tab" href="#Listed_jobs" aria-expanded="false">
                                                    Listed jobs
                                                    @if (Model.ListedJobCount != null)
                                                    {<span id="splc">(@Model.ListedJobCount)</span>}
                                                    else
                                                    {<span id="splc">(0)</span>}
                                                </a>
                                            </li>
                                            <li class="" id="ligigs">
                                                <a data-toggle="tab" href="#Listed_gigs" aria-expanded="false">
                                                    Listed GIGs
                                                    @if (Model.ListedGigCount != null)
                                                    {<span id="splg">(@Model.ListedGigCount)</span>}
                                                    else
                                                    {<span id="splg">(0)</span>}
                                                </a>
                                            </li>
                                        </ul>

                                    }
                                }


                                <div class="tab-content">
                                    @if (Model.Profile != null)
                                    {
                                        if (Model.Profile.UserType == 1)
                                        {
                                            <div id="completed_jobs" class="tab-pane fade clearfix active in">

                                                @if (Model.CompletedJobCount == 0 && Session["UserId"] != null && Convert.ToInt32(Session["UserId"]) != 0)
                                                {
                                                    @*<div class="col-md-12 category_nojob_bg clearfix">
                                                        <h4>You have not completed any jobs yet. To search jobs <a href="/jobs/index">click here</a>.</h4>
                                                    </div>*@
                                                        <div class="nojob_diplytable">
                                                              <div class="col-md-12 category_nojob_bg clearfix">
                                                              <h3>You have not completed any jobs yet.</h3>
                                                              <a href = "/jobs/index" class="btn button_all secondary_color_bg">Search jobs</a>
                                                              </div> 
                                                        </div>
                                                }
                                                else
                                                {
                                                    if (Model.CompletedJob != null)
                                                    {
                                                        foreach (var item in Model.CompletedJob)
                                                        {
                                                            <div class="col-md-12 home_user_box_1_bg bg_color_comen clearfix child" id="@item.JobId">
                                                                <a href="@Url.Action("Details", "Jobs" , new { @id=@item.JobId })">

                                                                    <h3>@item.JobTitle</h3>
                                                                </a>
                                                                <div class="timeframe_budget_small clearfix">
                                                                    <div class="timeframe_only"><span>Deadline: </span>&nbsp;<span>@item.JobCompletionDateDisplay</span></div>
                                                                    <div class="budget_only"><span>Budget: </span>&nbsp;<span>@String.Concat(@item.BudgetASP.ToString("#,##0"), " AXPR ", "($", @item.DollarCount.ToString("#,##0.00"), ")")</span></div>
                                                                    <div class="axp_star_only">
                                                                        <ul>
                                                                            @if (item.BadgeCount == 1)
                                                                            {
                                                                                <li><i></i></li>
                                                                            }
                                                                            else if (item.BadgeCount == 2)
                                                                            {
                                                                                <li><i></i></li>
                                                                                <li><i></i></li>
                                                                            }
                                                                            else if (item.BadgeCount == 3)
                                                                            {
                                                                                <li><i></i></li>
                                                                                <li><i></i></li>
                                                                                <li><i></i></li>
                                                                            }
                                                                        </ul>
                                                                    </div>
                                                                </div>
                                                                <div class="home_client_discription_small clearfix">
                                                                    <p>@item.JobDescription</p>
