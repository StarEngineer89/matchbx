    $("#jobPostAdd").click(function () {

        var JobId = 0;
        $.ajax({
            type: 'POST',
            cache: false,
            url: '@Url.Action("PostJob", "Jobs")',
            dataType:'html',
            data: { "id": JobId },
            success: function (response) {
                $("#postjob_view_pop_id").html(response)
                openbox(1, 'postjob_view_pop_id');
