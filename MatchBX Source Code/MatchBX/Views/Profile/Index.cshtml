                    else {

                        $("#completed_jobs").empty();
                        var _Msg = "";
                        _Msg = "<div class='col-md-12 category_nojob_bg clearfix'><h3>No result Found.</h3></div>"
                        $("#completed_jobs").html(_Msg);
                    }

                },
                error: function (jqXHR, exception) {
                    console.log(jqXHR.status);
                    console.log(exception);
                }
            });
        }

