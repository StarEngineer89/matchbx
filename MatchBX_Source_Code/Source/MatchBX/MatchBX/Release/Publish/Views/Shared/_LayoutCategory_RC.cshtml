        /*Header menu Responsive*/


        $(function () {
            $(window).resize(function () {
                $("#ulcategory").append($("#ulcategory li.hideshow ul").html());
                $("#ulcategory li.hideshow").remove();
                if ($(window).width() >= 751 && $(window).width() <= 1199.98) {
                    console.log($(window).width());
                    alignMenu();
                }
            });


        });
        function alignMenu() {

            var w = 0;
            var mw = $("#ulcategory").width() - 50;
            var i = -1;
            var menuhtml = '';
            jQuery.each($("#ulcategory").children(), function () {
                i++;
                w += $(this).outerWidth(true);
                if (mw < w) {
                    menuhtml += $('<div>').append($(this).clone()).html();
                    $(this).remove();
                }
            });
            $("#ulcategory").append(
                '<li  style="position:relative;" href="#" class="hideshow">'
                + '<a href="#">More '
                + '<i class="fa fa-angle-down" aria-hidden="true"></i>'
                + '</a><ul>' + menuhtml + '</ul></li>');
            $("#ulcategory li.hideshow ul").css("top",
                $("#ulcategory li.hideshow").outerHeight(true) + "px");
            setTimeout(function () {
                $("#ulcategory").css("opacity", "1");

            }, 400);

            $("#ulcategory li.hideshow").click(function () {
                $(this).children("ul").toggle();
            });
        }

        $(document).ready(function () {
            if ($(window).width() >= 767.98 && $(window).width() <= 1199.98) {
                alignMenu();
            }
        })

        $(document).click(function (event) {
            if (!$(event.target).closest('.hideshow').length) {
                if ($('.hideshow ul').is(":visible")) {
                    $('.hideshow ul').hide();
                }
            }
        });
        //$(window).resize(onResize_menu);

        /* // Header menu Responsive*/

    </script>
