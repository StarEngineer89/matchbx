    <script type="text/javascript">

        $(document).ready(function(){
            var Url='@(System.Configuration.ConfigurationManager.AppSettings["BaseURL"].ToString())'
            var userid = '@Session["UserId"].ToString()'
            var usertype = '@Session["UserType"].ToString()'
            App.init(Url,userid,usertype);
            $("#anchor_home").click(function(){

                localStorage.removeItem("frmPage");
               
                window.location.href="/Dashboard/Home";
            })
            function setCategory(_SortDirCat)
            {
                $.ajax({
                    type: 'POST',
                    cache: false,
                    url: '@Url.Action("SetSearchCategory", "category")',
                    data: {"cat":_SortDirCat},
                    success: function (response) {
                        SetSearchCriteria();
                    },
                    error: function (jqXHR, exception) {

                        console.log(jqXHR.status);
                        console.log(exception);
                    }
                });
            }
            $("#ulcategory").on('click','a',function(){

                var _id=$(this).prop("id");
                //if(_id!="")
                //{

                //    var _id=$(this).prop("id");
                //    localStorage.setItem("catid",_id);
                //    var _text=titleCase($(this).text());
                //    localStorage.setItem("cattext",_text)
                //    window.location.href="/Category/index?id="+_id;
                //}
                if(_id=="view_jobs")
                {
                    setCategory('J');
                    window.location.href="/Jobs/index";
                }
                else if(_id=="view_gigs")
                {
                    setCategory('G');
                    window.location.href="/Gigs/search";
                }
            })

            $("#ulcategoryfooter").on('click','a',function(){
                var _id=$(this).prop("id");
                localStorage.setItem("catid",_id);
                var _text=titleCase($(this).text());
                localStorage.setItem("cattext",_text)
                setCategory('J');
                window.location.href="/Category/index?id="+_id+"&type=1";
            })
            $("#ulcategoryfootergig").on('click','a',function(){
                var _id=$(this).prop("id");
                localStorage.setItem("catid",_id);
                var _text=titleCase($(this).text());
                localStorage.setItem("cattext",_text)
                setCategory('G');
                window.location.href="/Category/index?id="+_id+"&type=2";
            })
            $("#ulfootertopcat").on('click','a',function(){
                var _id=$(this).prop("id");
                localStorage.setItem("catid",_id);
                var _text=titleCase($(this).text());
                localStorage.setItem("cattext",_text)
                window.location.href="/Category/index?id="+_id+"&type=1";
            })
            function titleCase(str) {
                str = str.toLowerCase();
                str = str.split(' ');
                for (var i = 0; i < str.length; i++) {
                    str[i] = str[i].charAt(0).toUpperCase() + str[i].slice(1);
                }
                return str.join(' ');
            }
        })
    </script>

