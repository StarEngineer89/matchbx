                        {
                            var _sendUserId = @Session["UserId"];
                            var _receiverUserId = @Session["UserId"];
                            var _fullname = "@@" + "@Session["UserName"]";
                            var _header = "Set up metamask";
                            var _notification = "Please make sure your metamask is linked to your AXPR wallet address when using MatchBX";
                            notification.server.send(_sendUserId, _receiverUserId, _fullname, _notification, _header,'');
                            openbox(1, "metamask_pop_id");
                        }
                        else if (check == "A")
                        {

                            var userid = @Session["UserId"];
                            var Hash = localStorage.getItem("Address");

                            function callback(addr) {
                                if (addr == 'error' || addr == '') {
                                    openbox(1, 'metamask_comfirmation_id');
                                } else {

                                    function balCallBack(res) {
                                        if (res == 'empty') {
                                            openbox(1, "metamaskempty");
                                        } else if (res == 'ethempty') {
                                            openbox(1, "ethempty");
                                        } else if (res == 'isbalance') {
                                            function callback(result) {
                                                if (result == 'cancelled') {
                                                    UpdateApprovalTransaction(userid, Hash);
                                                    $('#metamask').addClass('header_loged_menua_metamask');
                                                    $('.balance').html('<span>Authorize</span><span>MetaMask</span>');
                                                    localStorage.setItem("Metamask", "A");
                                                }
                                                else {

                                                }
                                            }
                                            $('#metamask').addClass('header_loged_menua_metamask');
                                            $('.balance').html('<span>Authorisation </span><span>pending</span>');
                                            $('#metamask').attr('onclick', '');
                                            localStorage.setItem("Metamask", "P");
                                            SaveTransactionDetail(0, userid, 'A', 'A', '', 0, Hash);
                                            App.handleApprove(callback);
                                        } else if (res == 'isApproved') {
                                            $('#metamask').addClass('header_loged_menua_metamask');
                                            $('.balance').html('<span>Reset</span><span>Wallet</span>');
                                            App.resetApproveBalance(function (result) {
                                                console.log(result);
                                            });
                                        }
                                    }


                                    App.checkBalance(balCallBack);

                                }
                            }

                            App.getUserAddress(callback);



                        }
                        else
                        {

                        }
                    }
                    else
                    {

                    }
                });

