        function alignMenu() {
            var w = 0;
            var mw = $("#ulcategory").width() - 50;
            var i = -1;
            var menuhtml = '';
            jQuery.each($("#ulcategory").children(), function () {
                i++;
                w += $(this).outerWidth(true);
                if (mw < w) {
                    menuhtml += $('<div>').append($(this).clone()).html();
                    $(this).remove();
                }
            });
            $("#ulcategory").append(
                    '<li  style="position:relative;" href="#" class="hideshow">'
                            + '<a href="#">More '
                            + '<i class="fa fa-angle-down" aria-hidden="true"></i>'
                            + '</a><ul>' + menuhtml + '</ul></li>');
            $("#ulcategory li.hideshow ul").css("top",
                    $("#ulcategory li.hideshow").outerHeight(true) + "px");
            setTimeout(function(){
                $("#ulcategory").css("opacity","1");

            }, 400);
            $("#ulcategory li.hideshow").click(function () {
                $(this).children("ul").toggle();
            });
        }

