            chat.client.addNewMessageToPage = function ( message, receiverUserId, sendUserId,type,OnlineStatus,roomId,Messagetype,filesize,filename) {


                var _ReceiverId =@Session["UserId"];
                var _hdnRoomId=$("#hdnRoomId").val();
                const monthNames = ["Jan", "Feb", "Mar", "Apr", "May", "Jun",
                                    "Jul", "Aug", "Sep", "Oct", "Nov", "Dec"];
                if(_hdnRoomId==roomId){
                    if(receiverUserId==_ReceiverId ||  sendUserId==_ReceiverId){
                        if(type=="Chat"){


                            var d = new Date();
